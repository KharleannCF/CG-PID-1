<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="379.0" prefWidth="369.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="pditareajavafx.FXMLDocumentController">
    <children>
        <Button fx:id="cargar" layoutX="14.0" layoutY="340.0" onAction="#loadImage" text="Cargar" />
      <ImageView fx:id="imgV" fitHeight="311.0" fitWidth="341.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="negative" layoutX="74.0" layoutY="340.0" mnemonicParsing="false" onAction="#negative" text="Negativo" />
      <Button fx:id="grayScale" layoutX="147.0" layoutY="340.0" mnemonicParsing="false" onAction="#blackAndWhite" text="GrayScale" />
      <Button fx:id="borders" layoutX="223.0" layoutY="340.0" mnemonicParsing="false" onAction="#Borders" text="Bordes" />
    </children>
</AnchorPane>
